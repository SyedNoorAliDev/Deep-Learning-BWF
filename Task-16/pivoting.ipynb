    "\n",
    "# Create an example data frame\n",
    "data = pd.DataFrame({'key': ['foo', 'bar', 'baz', 'foo', 'bar', 'baz'],\n",
    "                     'value': [1, 2, 3, 4, 5, 6]})\n",
    "\n",
    "# Pivot the data frame from \"long\" to \"wide\" format\n",
    "pivoted = data.pivot('key', 'value')\n",
    "\n",
    "print(pivoted)\n"
